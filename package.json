{
    "name": "rui-next",
    "version": "0.29.0",
    "description": "Mobile web UI components based on React and Vite 2",
    "files": [
        "dist/style.css",
        "dist/rui-next.es.js",
        "dist/rui-next.umd.js",
        "CHANGELOG.md",
        "LICENSE",
        "README.md"
    ],
    "main": "dist/rui-next.umd.js",
    "module": "dist/rui-next.es.js",
    "exports": {
        ".": {
            "import": "./dist/rui-next.es.js",
            "require": "./dist/rui-next.umd.js"
        },
        "./css": "./dist/style.css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/v-poc/rui-next.git"
    },
    "keywords": [
        "vite",
        "react",
        "react-hooks",
        "vitepress",
        "react-components",
        "react-vitepress",
        "react-hooks-components",
        "react-hooks-vitepress",
        "rui",
        "react-ui",
        "react-hooks-ui",
        "react-vite",
        "react-vite-ui",
        "react-vite-vitepress",
        "react-vite-vitepress-ui",
        "vite2",
        "react-vite2-ui",
        "react-vite2-vitepress-ui"
    ],
    "author": "Niko Ni",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/v-poc/rui-next/issues"
    },
    "homepage": "https://github.com/v-poc/rui-next/blob/main/README.md",
    "bin": {
        "vitepress-rc": "bin/index.js"
    },
    "scripts": {
        "dev": "run-p dev-client dev-node",
        "dev-client": "tsc -w -p src/client",
        "dev-node": "tsc -w -p src/node",
        "build": "rimraf -rf dist && tsc -p src/client && tsc -p src/node",
        "docs": "run-p dev docs-dev",
        "docs-dev": "node ./bin/index.js dev docs",
        "docs-build": "npm run build && node ./bin/index build docs",
        "docs-serve": "node ./bin/index serve docs",
        "lint": "prettier --write ./src/**/* && eslint --fix",
        "start": "npm run build && npm run docs-dev",
        "build-lib": "vite build --config lib/vite.config.ts",
        "build-docs": "npm run build && node ./bin/index build docs"
    },
    "engines": {
        "node": ">=12.2.0"
    },
    "lint-staged": {
        "**/*.(js|jsx|ts|tsx)": [
            "prettier --write ./src/**/*",
            "eslint --fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.8",
        "@types/hast": "^2.3.1",
        "@types/mdast": "^3.0.3",
        "@types/minimist": "^1.2.1",
        "@types/node": "^14.14.35",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/react-router-dom": "^5.1.7",
        "@types/styled-components": "^5.1.9",
        "husky": "^5.2.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "react": "^17.0.2 || ^16.14.0",
        "react-dom": "^17.0.2 || ^16.14.0",
        "react-is": "^17.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.45.2",
        "v-qr-code-next": "^0.1.8",
        "@mdx-js/mdx": "2.0.0-next.9",
        "@mdx-js/react": "2.0.0-next.9",
        "@vitejs/plugin-react": "^1.1.0",
        "chalk": "^4.1.0",
        "classnames": "^2.3.1",
        "compression": "^1.7.4",
        "copy-text-to-clipboard": "^3.0.1",
        "esbuild": "^0.14.1",
        "escape-html": "^1.0.3",
        "fs-extra": "^9.1.0",
        "globby": "^11.0.3",
        "less": "^4.1.1",
        "mdast-util-from-markdown": "0.8.5",
        "mdast-util-mdx": "0.1.1",
        "micromark-extension-mdxjs": "0.3.0",
        "minimist": "^1.2.5",
        "normalize.css": "^8.0.1",
        "ora": "^5.4.0",
        "polka": "^0.5.2",
        "prism-react-renderer": "^1.2.1",
        "react-docgen-typescript": "^1.21.0",
        "react-simple-code-editor": "^0.11.0",
        "remark-emoji": "2.2.0",
        "remark-frontmatter": "3.0.0",
        "remark-gfm": "1.0.0",
        "remark-parse-yaml": "^0.0.3",
        "remark-slug": "6.1.0",
        "sirv": "^1.0.11",
        "slash": "^3.0.0",
        "styled-components": "^5.2.1",
        "typescript": "^4.2.4",
        "unified": "^9.2.1",
        "unist-util-find": "^1.0.2",
        "unist-util-visit": "^2.0.3",
        "vite": "^2.7.0-beta.10"
    }
}
